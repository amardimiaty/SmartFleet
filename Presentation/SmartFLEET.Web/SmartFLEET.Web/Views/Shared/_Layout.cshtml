<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - SMART FLEET</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body ng-app="smart-fleet"  class="hold-transition skin-blue-light  sidebar-mini">
    @*@RenderPage("_menu.cshtml")*@

    @RenderBody()

    <link href="~/Scripts/leafjs/leaflet.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-notifications.min.css" rel="stylesheet" />
    <link href="~/Content/skywalk-docs.min.css" rel="stylesheet" />
<link href="~/Content/icofont.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://npmcdn.com/leaflet@1.0.0-rc.1/dist/leaflet.css">
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
<script src="~/Scripts/jq-layout.js"></script>
    <script src="~/Scripts/leafjs/leaflet-providers.js"></script>
    <link href="~/Scripts/easyui/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/dist/css/skins/_all-skins.min.css" rel="stylesheet" />
    <link href="~/dist/css/AdminLTE.min.css" rel="stylesheet" />
<link href="~/Scripts/easyui/themes/color.css" rel="stylesheet" />
    <link href="~/Scripts/easyui/themes/icon.css" rel="stylesheet" />
    @RenderSection("scripts", required: false)

<script>
   
    $(document).ready(function () {

        layout  = $('body').layout({
            applyDemoStyles: true,
            onresize: function () {
            }
        });
        layout.toggle("west");
        // initialize the map


    });
</script>
</body>
</html>
